                        <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Experiment Path" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="experimentPathText" editable="false" onMouseClicked="#handlePathButton" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Button fx:id="experimentPathButton" mnemonicParsing="false" onAction="#handlePathButton" text="Folder..." GridPane.columnIndex="2" GridPane.rowIndex="2" />
                        <Label text="Model File" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <TextField fx:id="modelFileText" editable="false" onMouseClicked="#handleModelFileButton" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Button fx:id="modelFileButton" mnemonicParsing="false" onAction="#handleModelFileButton" text="File..." GridPane.columnIndex="2" GridPane.rowIndex="4" />
                        <TextField fx:id="rsiFileText" editable="false" onMouseClicked="#handleRSIFileButton" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                        <Button fx:id="rsiFileButton" mnemonicParsing="false" onAction="#handleRSIFileButton" text="File..." GridPane.columnIndex="2" GridPane.rowIndex="6" />
                        <Label text="RSI File" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                     </children>
                  </GridPane>
               </content>
            </Tab>
            <Tab text="Settings">
               <content>
                  <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="100.0" />
